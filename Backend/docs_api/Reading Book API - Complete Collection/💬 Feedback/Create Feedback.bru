meta {
  name: Create Feedback
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/feedback
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
  Authorization: Bearer {{authToken}}
}

body:json {
  {
    "bookId": "{{bookId}}",
    "rating": 5,
    "comment": "Cuốn sách rất hay và bổ ích!",
    "userId": "{{userId}}"
  }
}

settings {
  encodeUrl: true
}
